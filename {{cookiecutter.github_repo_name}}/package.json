    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "npm-run-all": "^4.1.1",
    "prettier-eslint-cli": "^4.4.0"
  },
  "repository": "{{ cookiecutter.github_username }}/{{ cookiecutter.github_repo_name }}",
